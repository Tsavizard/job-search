{
  "name": "@karriera-test/job-search",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "node-env=development"
            ]
          }
        }
      },
      "start-docker": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "apps/job-search/tools/scripts/dev.sh"
          ],
          "envFile": "apps/job-search/envs/.env.development",
          "parallel": false
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build",
          "start-docker"
        ],
        "options": {
          "buildTarget": "@karriera-test/job-search:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@karriera-test/job-search:build:development"
          },
          "production": {
            "buildTarget": "@karriera-test/job-search:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      },
      "docker-build": {
        "dependsOn": [
          "build"
        ],
        "command": "docker build -f docker/job-search.Dockerfile . -t job-search"
      }
    }
  }
}
