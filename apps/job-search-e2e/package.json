{
  "name": "@karriera-test/job-search-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@karriera-test/job-search"
    ],
    "targets": {
      "pre-e2e": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "docker exec -i karriera-job-search-elastic mysql -u \"$MYSQL_USER\" -p\"$MYSQL_PASSWORD\" job-search < docker/elastic/truncate.sh",
            "docker exec -i karriera-job-search-elastic mysql -u \"$MYSQL_USER\" -p\"$MYSQL_PASSWORD\" job-search < docker/elastic/seed.sh"
          ],
          "envFile": "envs/.env.elastic.development",
          "parallel": false
        }
      },
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/job-search-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@karriera-test/job-search:build"
        ]
      }
    }
  }
}
